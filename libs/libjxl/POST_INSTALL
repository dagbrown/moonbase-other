# Deal with soname versioning.

if module_installed gdal; then
  lunar fix gdal
fi

if module_installed ImageMagick; then
  lunar fix ImageMagick
fi

if module_installed kimageformats; then
  lunar fix kimageformats
fi

if module_installed imlib2; then
  lunar fix imlib2
fi

if module_installed darktable; then
  lunar fix darktable
fi
