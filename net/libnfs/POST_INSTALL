# Deal with some soname version changes.
if module_installed mpd; then
  lunar fix mpd
fi

if module_installed vlc; then
  lunar fix vlc
fi

if module_installed qemu; then
  lunar fix qemu

if module_installed gvfs; then
  lunar fix gvfs
fi

if module_installed xine-lib; then
  lunar fix xine-lib
fi
