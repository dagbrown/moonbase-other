          SOURCE=$MODULE-$VERSION.tar.gz
 SOURCE_URL_FULL=http://github.com/paramiko/paramiko/archive/$VERSION.tar.gz
      SOURCE_VFY=sha256:8a59d7085ed0adf8a6254e5b96c9f99249739ded1c7beddecf0c37cc28b3cca4
