         MODULE=libdvdread
        VERSION=6.1.2
         SOURCE=$MODULE-$VERSION.tar.bz2
     SOURCE_URL=https://download.videolan.org/pub/videolan/libdvdread/$VERSION/
     SOURCE_VFY=sha256:cc190f553758ced7571859e301f802cb4821f164d02bfacfd320c14a4e0da763
