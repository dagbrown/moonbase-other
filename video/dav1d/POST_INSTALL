if module_installed libavif; then
  lunar fix libavif
fi

if module_installed ffmpeg; then
  lunar fix ffmpeg
fi

if module_installed ffmpeg-4; then
  lunar fix ffmpeg-4
fi

if module_installed libheif; then
  lunar fix libheif
fi

if module_installed vlc; then
  lunar fix vlc
fi

