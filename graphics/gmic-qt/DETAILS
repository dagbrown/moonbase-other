          SOURCE=gmic_${VERSION}.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/gmic-$VERSION
      SOURCE_URL=https://gmic.eu/files/source/
      SOURCE_VFY=sha256:9f053338752ec96a6b619718037767682c5fd58e2471c08f3740fdb070605bc0
