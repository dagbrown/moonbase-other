          MODULE=libpng
         VERSION=1.6.43
          SOURCE=$MODULE-$VERSION.tar.xz
   SOURCE_URL[0]=$SFORGE_URL/$MODULE
   SOURCE_URL[1]=https://fossies.org/linux/misc/
