          MODULE=dosbox-x
         VERSION=2024.10.01
          SOURCE=$MODULE-v$VERSION.tar.gz
 SOURCE_URL_FULL=https://github.com/joncampbell123/dosbox-x/archive/refs/tags/dosbox-x-v${VERSION}.tar.gz
      SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$MODULE-v$VERSION
