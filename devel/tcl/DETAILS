        SOURCE_URL=$SFORGE_URL/$MODULE
       SOURCE_VFY=sha256:5880225babf7954c58d4fb0f5cf6279104ce1cd6aa9b71e9a6322540e1c4de66
          WEB_SITE=https://tcl.sourceforge.net
