 SOURCE_URL_FULL=https://github.com/nachoparker/dutree/archive/v${VERSION}.tar.gz
      SOURCE_VFY=sha256:55c30e57cc339dd16141510af33245cc3b82f588f22419fc034f02b36ebecba0
