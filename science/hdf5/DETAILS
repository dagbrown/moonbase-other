          MODULE=hdf5
         VERSION=1.14.0
          SOURCE=CMake-$MODULE-$VERSION.tar.gz
 SOURCE_URL_FULL=https://github.com/HDFGroup/hdf5/archive/refs/tags/hdf5-1_14_0.tar.gz
